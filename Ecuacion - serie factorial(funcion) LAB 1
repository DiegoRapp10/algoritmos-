so
float serie (float x, int N)
{ 
	int sig,i; float s;
	sig=1;
	s=0;
	     for(int i=1;i<=N;i++)
		 {
			 s=s+sig*pow(x,i)/FAC(i);
			 sig=sig*(-1);
		 }
		 return (s);
}
int FAC(int a)
{
	int b=1;
	for(int i=1;i<=a;i++)
		b=b*i;
	return (b);
}
