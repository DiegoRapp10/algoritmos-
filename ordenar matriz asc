OrdAsc_Filas void (float Matriz [FILA] [COL], int n, int m)
{
    int i, j, k;
    aux flotador;
    for (i = 0; i <n; i ++)
        for (j = 0; j <m; j ++)
            de (k = j; j <m; j ++)
                si (Matriz [i] [j]> Matriz [i] [k])
                {
                    aux = Matriz [i] [j];
                    Matriz [i] [j] = Matriz [i] [k];
                    Matriz [i] [k] = aux;
                }

    
    
    
}
